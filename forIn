/* forIn biasanya digunakan untuk mengiterasi sebuah objek, sebenarnya bisa untuk array tetapi lebih bagus digunakan untuk objek (array bisa menggunakan forOf). forIn akan mengiterasi attribut sebuah objek */ 


let person = {
    firstName: "Muhammad",
    middleName: "Yusuf",
    lastName: "Aldiansyah"
}

for (const me in person) {
    console.log(`${me} : ${person[me]}`)
}
