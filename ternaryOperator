const nilai = 77
let ucapan
/*------------Normal If statement---------------------*/
if (nilai >= 75) {
    ucapan = "selamat anda lulus"
}
else{
    ucapan = "silahkan coba lagi"
}
console.log(ucapan)
/*--------------Ternary Operator-------------------*/

ucapan = nilai >= 75 ? "selamat anda lulus" : "silahkan coba lagi"
console.log(ucapan)
